@model IEnumerable<Product_CQRS.Models.Order>

<h2>All Orders Made</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Product Name</th>
            <th scope="col">Price Bought</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductName</td>
                <td>@product.ProductPrice</td>
            </tr>
        }

    </tbody>
</table>
<a asp-controller="Home" class="btn btn-danger w-25">Go Back</a>


